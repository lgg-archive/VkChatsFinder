<Window x:Name="cap_win" x:Class="VkChatsFinder.captcha"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Height="235.052" Width="261.855" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" ResizeMode="NoResize" Icon="icon.ico">
    <Grid>
        <TextBox x:Name="cap_text" HorizontalAlignment="Center" Height="23" Margin="10,119,10,65" TextWrapping="Wrap" VerticalAlignment="Center" Width="236" TabIndex="1" Panel.ZIndex="-1" KeyDown="cap_text_KeyDown"/>
        <Button x:Name="cap_go" Content="Enter" HorizontalAlignment="Left" Height="37" Margin="10,158,0,0" VerticalAlignment="Top" Width="236" Click="cap_go_Click"/>
        <Image x:Name="cap_img" HorizontalAlignment="Left" Height="94" Margin="10,10,0,0" VerticalAlignment="Top" Width="236" Source="{Binding}"/>

    </Grid>
</Window>
